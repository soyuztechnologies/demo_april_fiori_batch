{
    "sap.app":{
        "id": "com.ey.fin.ap",
        "type": "application",
        "ach": "FIN-ACC-AP",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "anubhavService": {
                "uri": "/sap/opu/odata/sap/ZAPR_MY_ODATA_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "supportedThemes": ["sap_fiori_3_dark","sap_horizon"],
        "deviceTypes":{
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "technology": "UI5"
    },
    "sap.ui5":{
        "dependencies": {
            "minUI5Version": "1.100.2",
            "libs": {
                "sap.m": {"minVersion": "1.100.2"},
                "sap.ui.core": {"minVersion": "1.100.2"},
                "sap.ui.layout": {"minVersion": "1.100.2"}
            }
        },
        "rootView":{
            "viewName": "com.ey.fin.ap.view.App",
            "type": "XML",
            "id": "idRoot"
        },
        "routing": {
            "config":{
                "viewPath": "com.ey.fin.ap.view",
                "viewType": "XML",
                "controlId": "appCon",
                "clearControlAggregation": true
            },
            "routes":[
                {
                    "name": "master",
                    "pattern": "",
                    "target":["donald","trump"]
                },{
                    "name": "detail",
                    "pattern": "fruits/{fruitId}",
                    "target":["donald", "modi"]
                },{
                    "name": "addProduct",
                    "pattern": "newProduct",
                    "target":["donald", "putin"]
                }
            ],
            "targets": {
                "donald":{
                    "viewName": "View1",
                    "viewId": "idView1",
                    "controlAggregation": "masterPages"
                },
                "trump":{
                    "viewName": "Empty",
                    "viewId": "idEmpty",
                    "controlAggregation": "detailPages"
                    
                },
                "modi": {
                    "viewName": "View2",
                    "viewId": "idView2",
                    "controlAggregation": "detailPages"
                    
                },
                "putin": {
                    "viewName": "Add",
                    "viewId": "idAdd",
                    "controlAggregation": "detailPages"
                    
                }
            }
        },
        "models": {
            "" :{
                "dataSource": "anubhavService",
                "settings": {
                    
                }
            },
            "old":{
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/mockdata/fruits.json",
              "settings": {}
            },
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleUrl": "i18n/i18n.properties"
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }


    }
}